@model MVC_JqGrid.Models.ASL_MENUMST

@{
    ViewBag.Title = "Delete";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    @Html.HiddenFor(model => model.MODULEID)

    <div class="container">
        <div class="row">
            <div class="col-md-2"></div>
            <div class="col-md-8">
                <div class="panel panel-info" style="padding: 0px; margin-bottom: 0px;">
                    <div class="panel-heading text-center">
                        <h4>Are you sure you want to delete this?</h4>
                    </div>
                </div>


                <div class="jumbotron">

                    <div class="form-group">


                        <div class="row">
                            <div class="col-md-3">
                                @Html.LabelFor(model => model.MODULENM, htmlAttributes: new { @class = "control-label" })
                            </div>
                            <div class="col-md-8">
                                @Html.TextBoxFor(model => model.MODULENM, htmlAttributes: new { @class = "form-control", data_val = false, @readonly = "readonly" })
                            </div>
                        </div>


                        <br />
                        <div class="row">
                            <div class="col-md-3"></div>
                            <div class="col-md-8">
                                <input type="submit" class="btn btn-info" value="Delete" />
                            </div>

                        </div>

                    </div>
                </div>

            </div>
            <div class="col-md-2"></div>
        </div>

    </div>

}
