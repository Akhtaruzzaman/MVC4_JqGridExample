@model MVC_JqGrid.Models.ASL_MENUMST

@{
    ViewBag.Title = "EditModuleList";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    @Html.HiddenFor(model => model.MODULEID)
    
   <div id="contentHeaderBox">
    <h1>Update Module Information</h1>
</div>
<br />
<div class="col-md-12">
    <div class="form-group">

        <div class="row">
            <div class="col-md-3">
                @Html.LabelFor(model => model.MODULENM, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-8">
                @Html.TextBoxFor(model => model.MODULENM, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.MODULENM, "", new { @class = "text-danger" })
            </div>
        </div>
        
        <br />
        <br />
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-8">
                <input type="submit" class="btn btn-info" value="Update" />
            </div>

        </div>     
    </div>
    </div> 
    
  

}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}



