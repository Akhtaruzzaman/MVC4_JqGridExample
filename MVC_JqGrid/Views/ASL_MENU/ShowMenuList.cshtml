@model IEnumerable<MVC_JqGrid.Models.ASL_MENU>

@{
    ViewBag.Title = "ShowMenuList";
   
}

<div id="contentHeaderBox">
    <h1>Menu List</h1>
</div>
<br />
<div class="col-md-12">
    <div class="table-responsive">
        <table class="table table-striped">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.MODULEID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MENUID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MENUNM)
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.MODULEID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MENUID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MENUNM)
                    </td>
                </tr>
            }

        </table>
    </div>

    </div>

